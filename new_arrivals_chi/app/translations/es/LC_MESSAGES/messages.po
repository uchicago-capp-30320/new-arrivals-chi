#: new_arrivals_chi/app/templates/vttc_info.html:30
#: new_arrivals_chi/app/templates/vttc_info.html:47
#: new_arrivals_chi/app/templates/vttc_info.html:69
msgid "State benefits for Illinois are detailed below"
msgstr "Los beneficios estatales para Illinois se detallan a continuación"

#: new_arrivals_chi/app/templates/vttc_info.html:32
#: new_arrivals_chi/app/templates/vttc_info.html:49
#: new_arrivals_chi/app/templates/vttc_info.html:71
msgid "Documents"
msgstr "Documentos"

#: new_arrivals_chi/app/templates/vttc_info.html:34
#: new_arrivals_chi/app/templates/vttc_info.html:73
msgid "HHS Certification Letter or Eligibility Letter"
msgstr "Carta de Certificación o Carta de Elegibilidad del HHS"

#: new_arrivals_chi/app/templates/vttc_info.html:35
msgid "Employment Authorization Document or Social Security number (SSN)"
msgstr "Documento de Autorización de Empleo o número de Seguro Social (SSN)"

#: new_arrivals_chi/app/templates/vttc_info.html:37
msgid "U-1 Nonimmigrant Status (U Visa)"
msgstr "Estatus de No Inmigrante U-1 (Visa U)"

#: new_arrivals_chi/app/templates/vttc_info.html:39
msgid "For victims of various crimes, including trafficking"
msgstr "Para víctimas de varios delitos, incluida la trata de personas"

#: new_arrivals_chi/app/templates/vttc_info.html:42
msgid ""
"Up to four years of temporary nonimmigrant status with a path to lawful "
"permanent residence and citizenship"
msgstr ""
"Hasta cuatro años de estatus de no inmigrante temporal con un camino hacia la "
"residencia permanente legal y la ciudadanía"

#: new_arrivals_chi/app/templates/vttc_info.html:46
msgid "Federal and state benefits and services"
msgstr "Beneficios y servicios federales y estatales"

#: new_arrivals_chi/app/templates/vttc_info.html:51
#: new_arrivals_chi/app/templates/vttc_info.html:74
msgid "Employment Authorization Document or SSN"
msgstr "Documento de Autorización de Empleo o SSN"

#: new_arrivals_chi/app/templates/vttc_info.html:53
msgid "Continued Presence (CP)"
msgstr "Presencia Continua (CP)"

#: new_arrivals_chi/app/templates/vttc_info.html:59
msgid ""
"Two-year authorization, renewable until case is resolved in increments of up to "
"two years"
msgstr ""
"Autorización de dos años, renovable hasta que se resuelva el caso en incrementos "
